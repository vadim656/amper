{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Товары",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string"
    },
    "Price": {
      "type": "integer"
    },
    "UID": {
      "type": "uid",
      "targetField": "Name"
    },
    "Descriptoin": {
      "type": "richtext"
    },
    "Img": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "kategorii_tovarovs": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "tovaries"
    },
    "Complektaciya": {
      "type": "richtext"
    },
    "SizeDelivery": {
      "type": "component",
      "repeatable": false,
      "component": "size.size"
    },
    "Stocks": {
      "type": "component",
      "repeatable": true,
      "component": "stocks.t-est"
    },
    "order": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::order.order",
      "inversedBy": "tovaries"
    }
  }
}
